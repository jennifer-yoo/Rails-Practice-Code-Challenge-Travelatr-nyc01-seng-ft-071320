<h1>Bloggers#new</h1>
<% if flash[:my_errors]%>
    <% flash[:my_errors].each do |error|%>
    <%= error %>
    <% end%>
    <% end%>

    <h1>New Blogger Form</h1>
    <%= form_for @blogger do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %><br>
         <%= f.label :age %>
        <%= f.number_field :age %><br>
         <%= f.label :bio %>
        <%= f.text_area :bio %><br>
        <%= f.submit %>
        <% end%>

